    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	Док.Ссылка,
	Док.ПометкаУдаления,
	Док.Номер,
	Док.Дата,
	Док.Проведен,
	Док.ВидЗаявки,
	Док.Количество,
	Док.КоличествоТранспорта,
	Док.Контрагент,
	Док.ТипКонтрагента,
	Док.СуммаПоДокументу,
	Док.ЦенаДоллар,
	Док.Цена,
	Док.Валюта,
	Док.Комментарий,
	Док.Организация,
	Док.Ответственный,
	ТЧТранспорт.ДатаЗакрытия,
	ТЧТранспорт.Транспорт КАК НомерКонтейнера,
	ЕСТЬNULL(РегСвФайлы.КоличествоФайлов, 0) КАК КоличествоФайлов,
	Док.ТранспортСтрокой как ТранспортСтрокой
ИЗ
	Документ.Заявка КАК Док
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.Заявка.Транспорт КАК ТЧТранспорт
		ПО Док.Ссылка = ТЧТранспорт.Ссылка
			И (ТЧТранспорт.НомерСтроки = 1)
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			Файлы.Объект КАК Объект,
			СУММА(1) КАК КоличествоФайлов
		ИЗ
			РегистрСведений.Файлы КАК Файлы
		ГДЕ
			Файлы.Объект ССЫЛКА Документ.Заявка
		
		СГРУППИРОВАТЬ ПО
			Файлы.Объект) КАК РегСвФайлы
		ПО Док.Ссылка = РегСвФайлы.Объект</queryText>
      <mainTable>Document.Заявка</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
